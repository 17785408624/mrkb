<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mrkb.dao.dao.StoreUserGradeMapper" >
<!-- 新增客户课程信息与用户等级关联信息 -->
    <insert id="addStoreUserGrade">
		insert into store_user_grade 
		(store_id,user_grade_id) 
		value (#{store_id,jdbcType=INTEGER}
		,#{user_grade_id,jdbcType=INTEGER}
		);
	</insert>
</mapper>